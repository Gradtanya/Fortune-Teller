<body>

<div class="container">
    <h1>Нехай ця цитата вас надихне!</h1>
    <p></p>
    <div id="prediction" class="prediction"> </div>
    <button class="button" onclick="generatePrediction()">Отримати своє натхнення</button>
</div>

<script>
    const predictions = [
        "Сьогодні буде ваш день!",
        "Очікуйте на важливі новини.",
        "Завтра ви зробите великий крок вперед.",
        "Нові можливості чекають на вас.",
        "Не бійтеся ризикувати сьогодні.",
        "День принесе багато радісних моментів.",
        "Ви на правильному шляху!",
        "Довіряйте своїй інтуїції.",
        "Сьогодні ви здивуєте себе.",
        "Тримайтеся за мрії — вони здійсняться."
    ];

    const backgroundImages = [
        'url("data:image/jpeg;base64,")',
        'url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTDZSzVrPGh8S18GOqkKB0Xj2eYpgJ0ZtnyrA&s")',
        'url("data:image/jpeg;base64,")',
        'url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTfZDOcgxvWSlJQjm0Cnf5nIBz--lriiuFhUw&s")',
        'url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTGaF2yfsQKgo7eqnK8MHhZo2JxlCtJZtlQuQ&s")'
    ];

    // Функція для генерації випадкового передбачення
    function generatePrediction() {
        // Вибір випадкового передбачення
        const randomPrediction = predictions[Math.floor(Math.random() * predictions.length)];

        // Вибір випадкової фонової картинки
        const randomImage = backgroundImages[Math.floor(Math.random() * backgroundImages.length)];

        // Встановлення нового передбачення та фону
        document.getElementById('prediction').textContent = randomPrediction;
        document.body.style.backgroundImage = randomImage;
    }
</script>

</body>
